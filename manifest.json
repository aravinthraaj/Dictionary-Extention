{
  "manifest_version":2,
  "name": "First Extention",
  "version":"0.1",
  "content_scripts":[
    {
      "matches":[
        "<all_urls>"
      ],
      "js":[
        "content.js"
      ]
    }
  ],
  "background":{
    "scripts":["background.js"]
  },
  
  "browser_action":{
    "default_icon":"sample.png",
    "default_popup":"sketch/index.html",
    "default_title":"A popup will come here."
  }
}